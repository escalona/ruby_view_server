<style type="text/css">
ul {
  list-style-type: none;
  margin: 0;
  padding: 0
}

li {
  display: inline;
}
</style>

<body>
<div class="container">
  <div class="nav">
    <ul>
      <% ['home', 'about', 'contact'].each do |link| %>
